<Page x:Class="Sudoku.LoadSavedGamePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Sudoku"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="LoadSavedGamePage">

    <Grid>
        <Frame x:Name="LoadSavedGameFrame" >
        </Frame>
        <StackPanel x:Name="LaodSavedGamePageContent">
            <Label Content="Choose a game to load.. "/>
            <ComboBox x:Name="CmbBoxSavedGames" SelectedValuePath="Content"/>
            <Button Name="BtnLoad" Content="Load" Click="BtnLoad_Click" />
        </StackPanel>
    </Grid>
</Page>
