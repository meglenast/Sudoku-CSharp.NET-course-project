<Window x:Class="Sudoku.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sudoku"
        mc:Ignorable="d"
       Title="MainWindow" Height="700" Width="750">
    <Grid>
        <StackPanel x:Name="BtnsMenu" Orientation="Horizontal" VerticalAlignment="Top" Margin="10,0,0,0">
            <Button x:Name="BtnNewGame" Content="NewGame" Margin="10" Click="Button_Click"/>
            <Button x:Name="BtnLoadGame" Content="LoadGame" Margin="10"/>
            <Button x:Name="BtnHighScore" Content="HighScore" Margin="10"/>
        </StackPanel>
        <Frame x:Name="Main" Margin="0,60,0,0">
        </Frame>
    </Grid>
</Window>
